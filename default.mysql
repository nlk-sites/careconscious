INSERT INTO `boxes` VALUES ('1','Lorem ipsum, etc.','Community Header Left','3'),('2','<h4 style=\"margin-top: 1em;\">Feel free to:</h4>\r\n<ul style=\"list-style-type: square !important; list-style-position: inside;\">\r\n<li>Read posts</li>\r\n<li>Join existing conversations</li>\r\n<li>Start new conversations of your own</li>\r\n</ul>\r\n\r\n<a href=\'/node/add/community\'>New Topic</a> <a href=\'/node/add/expert-q\'>Ask an expert</a>','Community Header Right (Member)','2'),('3','<?php\r\n$destination = __comm_guest_block_postproccess_destination_url(drupal_get_destination());\r\n\r\nfunction __comm_guest_block_postproccess_destination_url ($destination_plus_path_with_query) {\r\n  // urldecode, then remove \"destination=\", then split with ?-s\r\n  $parts = explode (\"?\", preg_replace(\"!^destination=!\",  \"\", urldecode($destination_plus_path_with_query) ) );\r\n\r\n  // should be equal to $_GET[\'q\'] from above\r\n  $path_wo_query = $parts[0];\r\n\r\n  $query = $parts[1];\r\n\r\n  if ($query != \"\") {$query = \"?\". $query;}\r\n\r\n  $alias = drupal_get_path_alias($path_wo_query);\r\n\r\n  return urlencode($alias . $query);\r\n}\r\n?>\r\n<div style=\"padding: 1em 5em 2em 0\">\r\n<span style=\"font-weight: bold;\"> Join the Family Caregiving Community Today.</span><br />\r\n<span style=\"font-style: italic;\">The moral support, encouragement and advice every caregiver needs and deserves.</span>\r\n<br />\r\n<br />\r\n</div>\r\n<a href=\'/user/register?destination=<?php print $destination ?>\'>New Topic</a> <a href=\'/user/register?destination=<?php print $destination ?>\'>Ask an expert</a>','Community Header Right (Guest)','4'),('4','<?php\r\n$destination = __login_block_postproccess_destination_url(drupal_get_destination());\r\n\r\nfunction __login_block_postproccess_destination_url ($destination_plus_path_with_query) {\r\n  // urldecode, then remove \"destination=\", then split with ?-s\r\n  $parts = explode (\"?\", preg_replace(\"!^destination=!\",  \"\", urldecode($destination_plus_path_with_query) ) );\r\n\r\n  // should be equal to $_GET[\'q\'] from above\r\n  $path_wo_query = $parts[0];\r\n\r\n  $query = $parts[1];\r\n\r\n  if ($query != \"\") {$query = \"?\". $query;}\r\n\r\n  $alias = drupal_get_path_alias($path_wo_query);\r\n\r\n  return urlencode($alias . $query);\r\n}\r\nif (!$GLOBALS[\'user\']->uid) {\r\n?>\r\n<p>\r\n<?php print l(t(\'Sign Up!\'),\'user/register\',array(\'query\'=>array(\'destination\'=>$destination),\'attributes\'=>array(\'class\'=>\'medGoldBtn\')));?>\r\n<?php print l(t(\'Sign In\'),\'user/login\',array(\'query\'=>array(\'destination\'=>$destination),\'attributes\'=>array(\'class\'=>\'medGoldBtn\')));?>\r\n</p>\r\n<?php } else {?>\r\n<?php print l(t(\'Log Out\'),\'logout\',array(\'attributes\'=>array(\'class\'=>\'medGoldBtn\')));?>\r\n<?php } ?>','Register/Login Buttons','4');
INSERT INTO `boxes` VALUES ('5',' 2011, CareConscious. All rights reserved | <a href=\"/terms-and-conditions\">Terms & Conditions</a> | <a href=\"/privacy-policy\">Privacy Policy</a>','Footer Copyright','3'),('6','<ul>\r\n<li>Help/support line: 1-866-590-2351</li>\r\n</ul>','Helpline','3'),('7','<div class=\"item-list\">\r\n  <ul>\r\n    <li class=\"views-row\">\r\n      <?php if ($GLOBALS[\'user\']->uid) {?>\r\n      <a href=\"http://careconscious.com/content/caregiving-phone-support\">Call our support line:</a><br />\r\n      <a href=\"http://careconscious.com/content/caregiving-phone-support\">1-866-590-2351</a>\r\n      <?php } else { ?>\r\n      <?php echo l(t(Enroll), \'user/register\'); ?> now for access to our 24-hour support line and more.\r\n      <?php } ?>\r\n    </li>\r\n  </ul>\r\n</div>','Need immediate help','4'),('8','<p>Our experts are here to tackle the toughest issues.</p>\r\n<p>Go on, <br />\r\n<a title=\"\" href=\"/node/add/expert-q\" class=\"ask_expert\">Ask an expert</a>\r\n</p>\r\n','Still have questions?','2'),('10','<?php if (!$GLOBALS[\'user\']->uid) {?>\r\n<p>Get access to the financial, legal and emotional guidance you need.</p>\r\n<?php } ?>\r\n<div class=\"principle_block\">\r\n  <?php print theme(\'image\', path_to_theme(). \'/imgs/eight_principles.png\'); ?>\r\n  <h2><?php print l(\'Principles of Healthy Family Caregiving Program\', \'node/554\', array(\'attributes\'=>array(\'style\' => \'font-weight: bold!important; font-size: 16px!important;\'))); ?></h2>\r\n</div>\r\n<?php if ($GLOBALS[\'user\']->uid) {?>\r\n<p>View your plan and complete the principles <?php print l(\'NOW &gt;\', \'user\', array(\'html\' => true)); ?></p>\r\n<?php } ?>\r\n<?php if (!$GLOBALS[\'user\']->uid) {?>\r\n<p>Get access to the financial, legal and emotional guidance you need.</p>\r\n<?php } ?>\r\n<?php if (!$GLOBALS[\'user\']->uid) {?>\r\n<a href=\"/user/login\"><img class=\"block_sign_up\" src=\"/imgs/sign_up.png\"></a>\r\n<?php } ?>','Family Caregivers','4'),('12','Have an immediate caregiving need or question?  Licensed geriatrics professionals are standing by\r\n24/7 to provide you with the assistance, support, and resources you need, NOW.  ','Care Line','2');
