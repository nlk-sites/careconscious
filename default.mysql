INSERT INTO `node_revisions` VALUES ('1','1','1','Home','','','','1322527383','2'),('3','3','33','This is a Community post','This is a test entry for the Community section.','This is a test entry for the Community section.','','1311341205','3'),('18','18','1','Program Registration','<div class=\"join_text\">\r\n<h2>Join the program!</h2>\r\n\r\nFor the best possible experience, please go ahead and answer these few questions.  This will get you started right away with the 8 Principles of Healthy Family Caregiving.\r\n\r\n*If you have an immediate need, our <strong><a href=\"/caregiving-phone-support\">Caregivers\' CareLine</a></strong> is manned 24/7 with licensed geriatric social workers who can provide you with the help and resources you need right away. <strong>Dial 866.590.5321</strong> to access the CareLine now. \r\n\r\n</div>\r\n\r\nNeed more info about the Program?  <a href=\"/8-principles-healthy-family-caregiving\">Check out this quick overview of the 8 Principles of Family Caregiving</a>\r\n\r\n','<div class=\"join_text\">\r\n<h2>Join the program!</h2>\r\n\r\nFor the best possible experience, please go ahead and answer these few questions.  This will get you started right away with the 8 Principles of Healthy Family Caregiving.\r\n\r\n*If you have an immediate need, our <strong><a href=\"/caregiving-phone-support\">Caregivers\' CareLine</a></strong> is manned 24/7 with licensed geriatric social workers who can provide you with the help and resources you need right away. <strong>Dial 866.590.5321</strong> to access the CareLine now. \r\n\r\n</div>\r\n\r','','1322079105','2'),('9','9','0','shammelMember','','','','1273365558','0'),('11','11','8','shammelCareProvider','','','','1277128567','0'),('12','12','1','Dave S - Admin','','','','1284739123','0'),('13','13','1','Test CP','','','','1276813977','0'),('14','14','1','This is a comment on a','This is a comment on a community post.','This is a comment on a community post.','','1273767012','3'),('264','264','2